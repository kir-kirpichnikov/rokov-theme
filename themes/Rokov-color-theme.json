{
  "name": "Rokov",
  "type": "dark",
  "colors": {
    "activityBar.background": "#090909",
    "activityBar.foreground": "#eee",
    "activityBar.border": "#444",
    "activityBarBadge.background": "#eee",
    "activityBarBadge.foreground": "#090909",
    "activityBar.inactiveForeground": "#444",

    "sideBar.background": "#111",
    "sideBar.foreground": "#bbb",
    "sideBar.border": "#444",
    "sideBarTitle.foreground": "#999",
    "sideBarSectionHeader.background": "#222",
    "sideBarSectionHeader.foreground": "#999",

    "editor.foreground": "#bbb",
    "editor.background": "#090909",
    "editorLineNumber.foreground": "#333",

    "titleBar.activeBackground": "#090909",
    "titleBar.border": "#444",

    "tab.inactiveBackground": "#111",
    "tab.inactiveForeground": "#999",
    "editorGroupHeader.tabsBackground": "#111",
    "tab.border": "#111",
    "tab.activeBorderTop": "#eee",

    "menu.background": "#111",
    "menu.selectionBackground": "#090909",
    "menu.selectionBorder": "#444"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic bold",
        "foreground": "#383838"
      }
    },
    {
      "name": "Tag, Function, Object, This",
      "scope": [
        "entity.name.tag.css",
        "meta.selector.css",
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "storage.type.js",
        "storage.type.function.js",
        "punctuation.definition.block.js",
        "variable.language.this.js"
      ],
      "settings": {
        "foreground": "#7578fd",
        "fontStyle": "bold"
      }
    },
    {
      "name": "KeyWord, OperatorKeyWord",
      "scope": ["keyword.control", "keyword.operator"],
      "settings": {
        "foreground": "#74e3ff"
      }
    },
    {
      "name": "Class",
      "scope": [
        "entity.other.attribute-name.class.css",
        "meta.attribute.class.html",
        "meta.template.expression.js",
        "support.variable.property.js"
      ],
      "settings": {
        "foreground": "#c5b34c"
      }
    },
    {
      "name": "Id",
      "scope": ["entity.other.attribute-name.id.css", "meta.attribute.id.html"],
      "settings": {
        "foreground": "#92c54f"
      }
    },
    {
      "name": "Media, Rule, FunctionDefinition",
      "scope": [
        "meta.brace.round.js",
        "punctuation.definition.parameters.begin.js",
        "punctuation.definition.parameters.end.js",
        "meta.at-rule.media.header.css",
        "keyword.control.at-rule.charset.css",
        "meta.definition.method.js",
        "meta.definition.function.js",
        "storage.type.function.arrow.js"
      ],
      "settings": {
        "foreground": "#c089ff"
      }
    },
    {
      "name": "Function Call",
      "scope": ["meta.function-call.js"],
      "settings": {
        "foreground": "#8163b8",
        "fontStyle": "italic"
      }
    },
    {
      "name": "MetaTag, PropetyValue, VariableParameter",
      "scope": [
        "meta.tag",
        "meta.property-value.css",
        "variable.parameter.js",
        "support.type.property-name.json.comments"
      ],
      "settings": {
        "foreground": "#d88d5b"
      }
    },
    {
      "name": "PropertyNumeric, Boolean, Array",
      "scope": [
        "constant.numeric.css",
        "constant.numeric.decimal.js",
        "constant.language.boolean.true.js",
        "constant.language.null.js",
        "constant.language.nan.js",
        "constant.language.undefined.js",
        "meta.brace.square.js",
        "support.constant.json"
      ],
      "settings": {
        "foreground": "#f76d8f"
      }
    },
    {
      "name": "String, URL",
      "scope": [
        "string.template.js",
        "string.quoted.double.html",
        "variable.parameter.url.css",
        "string.quoted.double.css",
        "string.quoted.single.js",
        "string.quoted.double.json.comments"
      ],
      "settings": {
        "foreground": "#4bad71",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Color",
      "scope": ["constant.other.color.rgb-value", "meta.function.color.css"],
      "settings": {
        "foreground": "#58d6df"
      }
    },
    {
      "name": "Text",
      "scope": [
        "meta.property-name.css",
        "variable",
        "meta.definition.variable.js"
      ],
      "settings": {
        "foreground": "#eee"
      }
    },
    {
      "name": "Object",
      "scope": ["variable.other.object.js"],
      "settings": {
        "foreground": "#9c9c9c"
      }
    },
    {
      "name": "Vendored, ObjectMember",
      "scope": [
        "support.type.vendored.property-name.css",
        "variable.other.property.js",
        "meta.object.member.js"
      ],
      "settings": {
        "foreground": "#9c9c9c"
      }
    },
    {
      "name": "Important",
      "scope": ["keyword.other.important.css"],
      "settings": {
        "foreground": "#ff4343",
        "fontStyle": "italic"
      }
    }
  ]
}
